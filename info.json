{
    "keyboard_name": "Dactyl-Manuform (4x6_cf)",
    "manufacturer": "tshort",
    "url": "",
    "maintainer": "jceb",
    "processor": "STM32F103",
    "bootloader": "stm32duino",
    "usb": {
        "vid": "0x5342",  // ascii SB
        "pid": "0x0001",
        "device_version": "0.0.2"
    },
    "diode_direction": "ROW2COL",
    // "debounce": 10,
    "matrix_pins": {
        "cols": ["A10", "A9", "A8", "B15", "B14", "B13"],
        "rows": ["B6", "B7", "B8", "B9", "A0", "A1"],
    },
    // Other pins in use: B10, B0, B1
    "split": {
        // Which pin carries serial between the two halves - link to TRS cable
        // Apparently needs to be a TX pin. B10 is 5V tolerant USART TX3
        "soft_serial_pin": "B10",
        "transport": {"protocol": "serial"},
        //  "transport": {"protocol": "serial_usart"},
        "usb_detect": {"enabled": true},
        // "bootmagic": {
        //     "matrix": [6, 5]
        // },
        // "handedness": {
            // Use these if I decide to solder-specify handedness - which I probably
            // should do tbh
            // "pin": "C13"
            // If this position in matrix is empty, treat as left.
            // On left board, thumb cluster is at end of column rather
            // than start, so matrix 5,0 is empty on left but used on right.
        //     "matrix_grid": ["A10", "B9"],
        // }
    },
    "features": {
        "bootmagic": false,
        "magic": false,
        "command": false,
        "console": true,
        "combo": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "tap_dance": true,
        "split_keyboard": true,
        "analog_driver": true,
        "joystick": true,
        "pointing_device": true,
        "lto": true,
        "dynamic_macro": true,
        "caps_word": true,
        // Docs say I can move this to info.json but compiler complains
        // "caps_word": {
        //     "enabled": true,
        //     "both_shifts_turns_on": true,
        //     "idle_timeout": 3000,
        // },
        "key_override": true,
        "dynamic_tapping_term": false,
    },
    "tapping": {
        "term": 200,
        "term_per_key": false,
        // "toggle": 2,
        // Enables This makes it easier for fast typists to use dual-function keys
        "permissive_hold": true,
    },
    "mousekey": {
        "interval": 15,
        "delay":       0,
        "time_to_max": 60,
        "max_speed":   7,
        "wheel_delay": 0,
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 1, "y": 0},
                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0},
                {"matrix": [0, 4], "x": 4, "y": 0},
                {"matrix": [0, 5], "x": 5, "y": 0},

                // Right half rows start at row 6
                {"matrix": [6, 0], "x": 11, "y": 0},
                {"matrix": [6, 1], "x": 12, "y": 0},
                {"matrix": [6, 2], "x": 13, "y": 0},
                {"matrix": [6, 3], "x": 14, "y": 0},
                {"matrix": [6, 4], "x": 15, "y": 0},
                {"matrix": [6, 5], "x": 16, "y": 0},

                {"matrix": [1, 0], "x": 0, "y": 1},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 2, "y": 1},
                {"matrix": [1, 3], "x": 3, "y": 1},
                {"matrix": [1, 4], "x": 4, "y": 1},
                {"matrix": [1, 5], "x": 5, "y": 1},

                {"matrix": [7, 0], "x": 11, "y": 1},
                {"matrix": [7, 1], "x": 12, "y": 1},
                {"matrix": [7, 2], "x": 13, "y": 1},
                {"matrix": [7, 3], "x": 14, "y": 1},
                {"matrix": [7, 4], "x": 15, "y": 1},
                {"matrix": [7, 5], "x": 16, "y": 1},

                {"matrix": [2, 0], "x": 0, "y": 2},
                {"matrix": [2, 1], "x": 1, "y": 2},
                {"matrix": [2, 2], "x": 2, "y": 2},
                {"matrix": [2, 3], "x": 3, "y": 2},
                {"matrix": [2, 4], "x": 4, "y": 2},
                {"matrix": [2, 5], "x": 5, "y": 2},

                {"matrix": [8, 0], "x": 11, "y": 2},
                {"matrix": [8, 1], "x": 12, "y": 2},
                {"matrix": [8, 2], "x": 13, "y": 2},
                {"matrix": [8, 3], "x": 14, "y": 2},
                {"matrix": [8, 4], "x": 15, "y": 2},
                {"matrix": [8, 5], "x": 16, "y": 2},

                {"matrix": [3, 0], "x": 0, "y": 3},
                {"matrix": [3, 1], "x": 1, "y": 3},
                {"matrix": [3, 2], "x": 2, "y": 3},
                {"matrix": [3, 3], "x": 3, "y": 3},
                {"matrix": [3, 4], "x": 4, "y": 3},
                {"matrix": [3, 5], "x": 5, "y": 3},

                {"matrix": [9, 0], "x": 11, "y": 3},
                {"matrix": [9, 1], "x": 12, "y": 3},
                {"matrix": [9, 2], "x": 13, "y": 3},
                {"matrix": [9, 3], "x": 14, "y": 3},
                {"matrix": [9, 4], "x": 15, "y": 3},
                {"matrix": [9, 5], "x": 16, "y": 3},

                // Thumb cluster top left
                {"matrix": [4, 3], "x": 3, "y": 4},
                {"matrix": [4, 4], "x": 4, "y": 4},
                {"matrix": [4, 5], "x": 5, "y": 4},
                // Thumb cluster top right
                {"matrix": [10, 0], "x": 11, "y": 4},
                {"matrix": [10, 1], "x": 12, "y": 4},
                {"matrix": [10, 2], "x": 13, "y": 4},

                // Thumb cluster bot left
                {"matrix": [5, 3], "x": 3, "y": 5},
                {"matrix": [5, 4], "x": 4, "y": 5},
                {"matrix": [5, 5], "x": 5, "y": 5},
                // Thumb cluster bot right
                {"matrix": [11, 0], "x": 11, "y": 5},
                {"matrix": [11, 1], "x": 12, "y": 5},
                {"matrix": [11, 2], "x": 13, "y": 5}
            ]
        }
    }
}
